<div class="container">
    <div class="page-header">
    <h1 class="text-uppercase text-primary">Journal view</h1>
    <%= link_to 'New Entry', new_log_path, class:'btn btn-success fr entry' %>
    </div>
</div>
<div class="container">
  <table>
    <thead>
      <tr>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @logs.each do |log| %>
        <tr>
          <td><%= link_to 'Show', log %></td>
          <td><%= link_to 'Edit', edit_log_path(log) %></td>
          <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>


